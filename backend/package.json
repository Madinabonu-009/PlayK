{
  "name": "play-kids-backend",
  "version": "1.0.0",
  "description": "Play Kids Kindergarten Backend API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:all": "node src/scripts/test-all.js",
    "lint": "eslint src --ext .js --fix",
    "seed": "node src/scripts/seed.js",
    "seed:clear": "node src/scripts/seed.js --clear",
    "backup": "node src/scripts/backup.js",
    "migrate": "node -e \"import('./src/migrations/index.js').then(m => m.default.runPending())\"",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.34.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "rate-limit-redis": "^4.3.1",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "fast-check": "^3.22.0",
    "jest": "^29.7.0"
  }
}
